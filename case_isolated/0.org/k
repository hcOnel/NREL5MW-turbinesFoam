FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $turbulentKE;

boundaryField
{
    outlet
    {
        type            inletOutlet;
        inletValue      uniform $turbulentKE;
        value           $internalField;
    }

    inlet
    {
        type            uniformFixedValue;
        uniformValue    constant $turbulentKE;
    }

    sides
    {
        type            slip;//kqRWallFunction;
        //value           uniform 0.0;
    }

    top
    {
        type            slip;//kqRWallFunction;
        //value           uniform 0.0;
    }

    bottom
    {
        type            slip;//kqRWallFunction;
        //value           uniform 0.0;
    }

    "proc.*"
    {
        type            processor;
    }
}


